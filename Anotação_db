CREATE TABLE livros (
    ID_livro int(11) NOT NULL PRIMARY KEY,
    Nome_livro varchar(255),
    Autor varchar(255),
    Data_Publi varchar(10),
    Gênero varchar(255)
);

CREATE TABLE leitores (
    ID_leitor int(11) NOT NULL PRIMARY KEY,
    RA varchar(9),
    Nome varchar(255),
    Data_Nascimento varchar(10),
    Endereço varchar(255),
    Sexo varchar(1)
);

